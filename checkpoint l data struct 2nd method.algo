ALGORITHM datastructuresN_2
VAR
    sum : INTEGER :=0;
    i ,j : INTEGER;
    arr1,arr2 : ARRAY_OF INTEGER[];
    

BEGIN
 READ arr1
 READ arr2
   FOR i FROM 0 TO arr1.length-1 STEP 1  DO
       
       FOR j FROM 0 TO arr2.length-1 STEP 1  DO
           IF (arr1[i]==arr2[j]) THEN
               sum:=sum+arr1[i]
               break;
           END_IF
       END_FOR

   END_FOR
 WRITE sum

END